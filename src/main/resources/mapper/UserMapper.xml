<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.olliego.wiki.dao.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.olliego.wiki.model.User">
        <id column="id" property="id"/>
        <result column="create_people" property="createPeople"/>
        <result column="create_time" property="createTime"/>
        <result column="modify_people" property="modifyPeople"/>
        <result column="modify_time" property="modifyTime"/>
        <result column="deleted" property="deleted"/>
        <result column="login_name" property="loginName"/>
        <result column="user_name" property="userName"/>
        <result column="password" property="password"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , create_people, create_time, modify_people, modify_time, deleted
        , login_name, user_name, password
    </sql>

</mapper>
